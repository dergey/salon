<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Specialization.
    -->
    <changeSet id="20191215110156-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="employee_id"
                                 baseTableName="specialization"
                                 constraintName="fk_specialization_employee_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee"/>

        <addForeignKeyConstraint baseColumnNames="service_id"
                                 baseTableName="specialization"
                                 constraintName="fk_specialization_service_id"
                                 referencedColumnNames="id"
                                 referencedTableName="service"/>

    </changeSet>
</databaseChangeLog>
