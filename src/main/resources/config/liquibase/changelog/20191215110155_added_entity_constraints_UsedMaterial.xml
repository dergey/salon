<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity UsedMaterial.
    -->
    <changeSet id="20191215110155-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="order_id"
                                 baseTableName="used_material"
                                 constraintName="fk_used_material_order_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_order"/>

        <addForeignKeyConstraint baseColumnNames="material_id"
                                 baseTableName="used_material"
                                 constraintName="fk_used_material_material_id"
                                 referencedColumnNames="id"
                                 referencedTableName="material"/>

        <addForeignKeyConstraint baseColumnNames="employee_id"
                                 baseTableName="used_material"
                                 constraintName="fk_used_material_employee_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee"/>

    </changeSet>
</databaseChangeLog>
